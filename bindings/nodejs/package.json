{
  "name": "@gorules/zen-engine",
  "version": "0.47.1",
  "main": "index.js",
  "types": "./index.d.ts",
  "license": "MIT",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "README.md"
  ],
  "packageManager": "yarn@1.22.22",
  "napi": {
    "name": "zen-engine",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl",
        "x86_64-pc-windows-msvc",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-apple-darwin"
      ]
    },
    "npmClient": "yarn"
  },
  "keywords": [
    "gorules",
    "zen-engine",
    "business rules engine",
    "rules engine",
    "rule engine",
    "bre",
    "rule",
    "rules",
    "engine",
    "decision",
    "decision table",
    "rust",
    "N-API",
    "napi-rs",
    "node-rs"
  ],
  "author": "GoRules <hi@gorules.io> (https://gorules.io)",
  "homepage": "https://github.com/gorules/zen",
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@napi-rs/cli": "^2.18.4",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "babel-jest": "^29.7.0",
    "express": "^5.1.0",
    "jest": "^29.7.0",
    "lerna": "6",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "bugs": {
    "url": "https://github.com/gorules/zen/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorules/zen.git"
  },
  "scripts": {
    "build": "napi build --dts temp.d.ts --platform --release",
    "build:debug": "napi build --platform --js index.js --dts index.d.ts",
    "watch": "cargo watch --ignore '{index.js,index.d.ts}' -- npm run build:debug",
    "test": "jest",
    "artifacts": "napi artifacts -d ../../artifacts",
    "prepublishOnly": "napi prepublish",
    "version": "napi version"
  }
}
