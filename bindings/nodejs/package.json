{
  "name": "@gorules/zen-engine",
  "version": "0.50.0",
  "main": "index.js",
  "browser": "browser.js",
  "types": "./index.d.ts",
  "license": "MIT",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "browser.js",
    "README.md"
  ],
  "packageManager": "yarn@1.22.22",
  "napi": {
    "binaryName": "zen-engine",
    "targets": [
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl",
      "x86_64-pc-windows-msvc",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "aarch64-apple-darwin",
      "wasm32-wasi-preview1-threads"
    ],
    "npmClient": "yarn",
    "wasm": {
      "browser": {
        "fs": false,
        "asyncInit": true
      }
    }
  },
  "keywords": [
    "gorules",
    "zen-engine",
    "business rules engine",
    "rules engine",
    "rule engine",
    "bre",
    "rule",
    "rules",
    "engine",
    "decision",
    "decision table",
    "rust",
    "N-API",
    "napi-rs",
    "node-rs"
  ],
  "author": "GoRules <hi@gorules.io> (https://gorules.io)",
  "homepage": "https://github.com/gorules/zen",
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@napi-rs/cli": "^3.4.1",
    "@napi-rs/wasm-runtime": "^1.0.7",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "babel-jest": "^30.2.0",
    "cross-env": "^10.1.0",
    "emnapi": "^1.7.0",
    "express": "^5.1.0",
    "jest": "^30.2.0",
    "lerna": "6.6.2",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "bugs": {
    "url": "https://github.com/gorules/zen/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gorules/zen.git"
  },
  "scripts": {
    "build": "napi build --dts temp.d.ts --platform --release",
    "build:debug": "napi build --platform --js index.js --dts index.d.ts",
    "watch": "cargo watch --ignore '{index.js,index.d.ts}' -- npm run build:debug",
    "test": "cross-env __ZEN_MOCK_UTC_TIME=2025-08-19T16:55:02.078Z jest",
    "artifacts": "napi artifacts -d ../../artifacts",
    "prepublishOnly": "napi prepublish",
    "version": "napi version"
  },
  "resolutions": {
    "form-data@^4.0.0": "4.0.4"
  }
}
